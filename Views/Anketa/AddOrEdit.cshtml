@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

    <script>
        function SetOptions() {
            if (($('#sel2 option:selected').text() == "Дневное")) {
                $('#sel3 option').remove();
                $('#sel3').prepend('<option value="Коммерческая деятельность">Коммерческая деятельность</option>');
                $('#sel3').prepend('<option value="Правоведение">Правоведение</option>');
                $('#sel3').prepend('<option value="Визуальный мерчендайзинг">Визуальный мерчендайзинг</option>');
                $('#sel3').prepend('<option value="Программное обеспечение информационных технологий">Программное обеспечение информационных технологий</option>');
                $('#sel3').prepend('<option value="Операционная деятельность в логистике">Операционная деятельность в логистике</option>');
                $('#sel3').prepend('<option value="Бухгалтерский учёт, анализ и контроль">Бухгалтерский учёт, анализ и контроль</option>');
                $('#sel3').prepend('<option value="0">Нет</option>');
            }
            if ($('#sel2 option:selected').text() == "Заочное") {
                $('#sel3 option').remove();
                $('#sel3').prepend('<option value="Коммерческая деятельность">Коммерческая деятельность</option>');
                $('#sel3').prepend('<option value="Бухгалтерский учёт, анализ и контроль">Бухгалтерский учёт, анализ и контроль</option>');
                $('#sel3').prepend('<option value="Правоведение">Правоведение</option>');
                $('#sel3').prepend('<option value="0">Нет</option>');
            }
            if ($('#sel2 option:selected').text() == "Заочное" && $('#sel1 option:selected').text() == "Базовое") {
                $('#sel3 option').remove();
                $('#sel3').prepend('<option value="0">Нет</option>');
            }

        }

        function SetOptions2() {
            if ($('#sel1 option:selected').text() == 'Базовое' && $('#sel3 option:selected').text() == 'Правоведение') {
                $('#sel4 option').remove();
                $('#sel4').prepend('<option value="Хозяйственно-правовая и кадровая работа">Хозяйственно-правовая и кадровая работа</option>');
                $('#sel4').prepend('<option value="0">Нет</option>');
            }

            if ($('#sel1 option:selected').text() == 'Базовое' && $('#sel3 option:selected').text() == 'Коммерческая деятельность') {
                $('#sel4 option').remove();
                $('#sel4').prepend('<option value="Товароведение продовольственных и непродовольственных товаров">Товароведение продовольственных и непродовольственных товаров</option>');
                $('#sel4').prepend('<option value="Информационное обеспечение бизнеса">Информационное обеспечение бизнеса</option>');
                $('#sel4').prepend('<option value="0">Нет</option>');
            }

            if ($('#sel1 option:selected').text() == 'Базовое' && $('#sel3 option:selected').text() == 'Бухгалтерский учёт, анализ и контроль') {
                $('#sel4 option').remove();
                $('#sel4').prepend('<option value="Бухгалтерский учет, анализ и контроль">Бухгалтерский учет, анализ и контроль</option>');
                $('#sel4').prepend('<option value="0">Нет</option>');
